          "$ref": "#/definitions/v1User"
        },
        "message": {
          "type": "string"
        }
      }
    },
    "v1GetUserResponse": {
      "type": "object",
      "properties": {
        "user": {
          "$ref": "#/definitions/v1User"
        },
        "message": {
          "type": "string"
        }
      }
    },
    "v1User": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "userType": {
          "type": "string"
        },
        "emailVerified": {
          "type": "boolean"
        },
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "authMethod": {
          "type": "string"
        },
        "resetPasswordToken": {
          "type": "string"
        },
        "emailVerifyToken": {
          "type": "string"
        },
        "resetPasswordTokenExpires": {
          "type": "string"
        },
        "activatedAt": {
          "type": "string",
          "format": "date-time"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "deletedAt": {
          "type": "string",
          "format": "date-time"
        }
      }
